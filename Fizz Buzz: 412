#Speed beats 73%

class Solution:
    def fizzBuzz(self, n):
        rtrn = []
        for i in range(n):
            if (i+1) % 3 != 0 and (i+1) % 5 != 0:
                rtrn.append(str(i+1))
            else:
                if (i+1) % 5 == 0 and (i+1) % 3 == 0: 
                    rtrn.append("FizzBuzz")
                elif (i+1) % 3 == 0:
                    rtrn.append("Fizz")
                else: rtrn.append("Buzz")
        return rtrn
